<!DOCTYPE html>
<html dir="ltr">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
   
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="keywords" />
          
    <meta name="description" />
         
    <meta name="robots" content="noindex,nofollow" />
    <title>Profit AdminLogin</title>
    
    <link rel="icon"
          type="image/png"
          sizes="16x16"
          href="../Content/images/icon.png" />
   
    <link href="../AdminContent/dist/css/style.min.css" rel="stylesheet" />
    
</head>

<body>
    @RenderBody()
   
   
    <script src="../AdminContent/assets/libs/jquery/dist/jquery.min.js"></script>
    
    <script src="../AdminContent/assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
   
    <script>
    $(".preloader").fadeOut();
       
        $("#to-recover").on("click", function () {
            $("#loginform").slideUp();
            $("#recoverform").fadeIn();
        });
        $("#to-login").click(function () {
            $("#recoverform").hide();
            $("#loginform").fadeIn();
        });</script>
</body>
</html>

